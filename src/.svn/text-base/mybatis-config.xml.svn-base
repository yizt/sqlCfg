<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
	<!-- 映射文件 将sql、POJO、dao层映射连接在一起，以可配置的方式来组织。从此SQL的写法与dao层无关了。 -->
<configuration>
	<typeAliases>
		<typeAlias alias="SQLAdapter" type="com.sitech.sqlcfg.util.SQLAdapter" />
		<typeAlias alias="TbSqlCfg" type="com.sitech.sqlcfg.domain.TbSqlCfg" />
		<typeAlias alias="TbParamCfg" type="com.sitech.sqlcfg.domain.TbParamCfg" />
		<typeAlias alias="TbQuerySqlCfg" type="com.sitech.sqlcfg.domain.TbQuerySqlCfg" />
		<typeAlias alias="TbExecsql" type="com.sitech.sqlcfg.domain.TbExecsql" />
	</typeAliases>
	<mappers>
		<mapper resource="com/sitech/sqlcfg/dao/OperateMapper.xml" />
		<mapper resource="com/sitech/sqlcfg/dao/SqlCfgMapper.xml" />
	</mappers>
</configuration> 
